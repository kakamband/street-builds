<h1> Welcome <%= @user.username %></h1>

<h3>Your Garage</h3>
<% @user.owned_cars.each do |c| %>
    <h2><%= link_to "#{c.year} #{c.make} #{c.model}", car_path(c)%></h2>
    <p>Engine: <%= c.engine %></p>
    <p>Drivetrain: <%= c.drivetrain %></p>
    <p>Suspension: <%= c.suspension %></p>
    <p>Wheels And Tires: <%= c.wheels_and_tires %></p>
<% end %>

<h3><%= link_to "Your Boards", user_boards_path(@user) %></h3>
