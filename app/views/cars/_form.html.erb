        <%= form_for @car do |f| %>
            <%= f.label :images, "Upload A Quality Image", class: "w3-large w3-left" %><br>
            <%= f.file_field :images, class: "w3-left" %><br><br>
            <%= f.label :year, "Year", class: "w3-left" %><%= %>
            <%= f.number_field :year, class: "w3-input w3-border" %><br>
            <%= f.label :make, nil, class: "w3-left" %>
            <%= f.text_field :make, class: "w3-input w3-border" %><br>
            <%= f.label :model, nil, class: "w3-left" %>
            <%= f.text_field :model, class: "w3-input w3-border" %><br>
            <%= f.label :engine, nil, class: "w3-left" %>
            <%= f.text_field :engine, class: "w3-input w3-border" %><br>
            <%= f.label :drivetrain, nil, class: "w3-left" %>
            <%= f.text_field :drivetrain, class: "w3-input w3-border" %><br>
            <%= f.label :suspension_and_brakes, nil, class: "w3-left" %>
            <%= f.text_field :suspension_and_brakes, class: "w3-input w3-border" %><br>
            <%= f.label :wheels_and_tires, "Wheels and Tires", class: "w3-left" %>
            <%= f.text_field :wheels_and_tires, class: "w3-input w3-border" %><br>
            <%= f.submit class: "green-outline w3-left" %>
        <% end %> 