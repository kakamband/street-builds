<%= link_to "Share Your Build", new_car_build_path %>

<% @car_builds.each do |c| %>
    <h2><%= link_to "#{c.year} #{c.make} #{c.model}", car_build_path(c)%></h2>
    <p>Engine: <%= c.engine %></p>
    <p>Drivetrain: <%= c.drivetrain %></p>
    <p>Suspension: <%= c.suspension %></p>
    <p>Wheels And Tires: <%= c.wheels_and_tires %></p>
    <hr>
<% end %>